ShortName: pdtest
StorageClass:
  FromFile: {{.StorageClassFile}}
DriverInfo:
  Name: csi-gcepd
  SupportedFsType:
    ext2:
    ext3:
    ext4:
    # The following FS types supported by GCE PD but
    # currently we do not test the CSI Driver on Ubuntu or Windows
    # xfs: XFS only available on Ubuntu
    # ntfs: NTFS only available on Windows
  Capabilities:
    persistence: true
    multipods: true
    fsGroup: true
    exec: true
    block: true
    topology: true
    # dataSource: true
    # RWX: true
  SupportedMountOption:
    debug:
    nouid32:
  SupportedSizeRange:
    min: 5Gi
